{
  "name": "custom_template",
  "type": "custom",
  "description": "Custom project template for {{project_name}}",
  "version": "1.0",
  "directory_structure": [
    "src/",
    "tests/",
    "docs/",
    "config/",
    ".codomyrmex/"
  ],
  "template_files": {},
  "workflows": [
    "custom-workflow"
  ],
  "required_modules": [
    "static_analysis"
  ],
  "optional_modules": [
    "data_visualization",
    "documentation"
  ],
  "default_config": {
    "project": {
      "name": "{{project_name}}",
      "type": "{{project_type}}",
      "version": "{{version}}",
      "created": "{{created_at}}"
    },
    "analysis": {
      "include_patterns": ["*.py"],
      "exclude_patterns": ["*/__pycache__/*", "*/node_modules/*"]
    }
  },
  "documentation_config": {
    "nested_docs": [
      "src/",
      "tests/",
      "docs/",
      "config/"
    ],
    "doc_links": {
      "enabled": true,
      "parent_link": true,
      "child_links": true
    }
  },
  "author": "",
  "created_at": "2025-01-15T10:00:00+00:00"
}

