# Codomyrmex Development Environment Configuration
# Copy this file to .env and customize for your development setup

# Logging Configuration
CODOMYRMEX_LOG_LEVEL=DEBUG
CODOMYRMEX_LOG_FILE=codomyrmex_dev.log
CODOMYRMEX_DEBUG=true

# LLM API Keys (optional - only needed for AI features)
# Get these from your respective AI service providers
# OPENAI_API_KEY=sk-your-openai-key-here
# ANTHROPIC_API_KEY=sk-ant-your-anthropic-key-here
# GOOGLE_API_KEY=AIzaSy-your-google-key-here

# Development Settings
CODOMYRMEX_DEV_MODE=true
CODOMYRMEX_AUTO_RELOAD=true

# Performance Settings (for development)
CODOMYRMEX_CACHE_ENABLED=false
CODOMYRMEX_PERFORMANCE_MONITORING=true

# Database (if using database features)
# DATABASE_URL=sqlite:///dev.db

# External Services (uncomment and configure as needed)
# REDIS_URL=redis://localhost:6379
# ELASTICSEARCH_URL=http://localhost:9200

# Security (development settings - not for production)
CODOMYRMEX_SECRET_KEY=dev-secret-key-change-in-production
CODOMYRMEX_CORS_ORIGINS=http://localhost:3000,http://localhost:8000

# Monitoring and Telemetry
CODOMYRMEX_TELEMETRY_ENABLED=true
CODOMYRMEX_METRICS_PORT=9090
