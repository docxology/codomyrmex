      "module": "static_analysis",
      "action": "analyze_code_quality",
      "parameters": {
        "path": "."
      },
      "dependencies": [],
      "timeout": null,
      "retry_count": 0,
      "max_retries": 3
    },
    {
      "name": "generate_insights",
