{
  "config_management": {
    "config_sources": [
      "config.yaml",
      "config_overrides.yaml"
    ],
    "schema": {
      "app": {
        "required": true,
        "type": "object"
      },
      "database": {
        "required": true,
        "type": "object"
      },
      "logging": {
        "required": false,
        "type": "object"
      }
    },
    "access_examples": [
      {
        "key": "app.name",
        "default": "Unknown App"
      },
      {
        "key": "database.host",
        "default": "localhost"
      },
      {
        "key": "logging.level",
        "default": "INFO"
      }
    ]
  },
  "app": {
    "name": "Codomyrmex Config Example",
    "version": "1.0.0",
    "environment": "development",
    "features": [
      "config_validation",
      "schema_enforcement",
      "migration_support"
    ]
  },
  "database": {
    "host": "localhost",
    "port": 5432,
    "name": "codomyrmex_db",
    "credentials": {
      "username": "codomyrmex",
      "password": "secret123"
    }
  },
  "logging": {
    "level": "INFO",
    "format": "json",
    "file": "logs/config_management.log"
  },
  "features": {
    "enabled": [
      "validation",
      "migration",
      "secrets_management"
    ],
    "disabled": [
      "caching",
      "encryption"
    ]
  },
  "output": {
    "format": "json",
    "file": "output/config_management_results.json",
    "include_details": true
  },
  "logging": {
    "level": "INFO",
    "file": "logs/config_management_example.log"
  }
}

